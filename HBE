	if CloseUI.BackgroundTransparency ~= 0 then
		CloseUI.BackgroundColor3 = getgenv().cbackground
		CloseUI.BackgroundTransparency = 0
	end
end)

CloseYes.MouseButton1Click:Connect(function()
	for _, v in pairs(Topbar:GetChildren()) do
		if v.Name ~= "Type" and v.Name ~= "UICorner" then
			v.Visible = false
		end
	end
	game.TweenService:Create(ConfirmClose, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
		BackgroundTransparency = 1
	}):Play()
	wait()
	game.TweenService:Create(Topbar, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		Size = UDim2.new(0,0,0,0),
		Position = UDim2.new(0, Topbar.AbsolutePosition.X + (Topbar.AbsoluteSize.X / 2), 0, Topbar.AbsolutePosition.Y + (Topbar.AbsoluteSize.Y / 2))
	}):Play()
	wait(.1)
	ScreenGui:Destroy()
end)
CloseNo.MouseButton1Click:Connect(function()
	ConfirmClose.Visible = false
end)
--hover
