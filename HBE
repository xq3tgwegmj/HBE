Settings.Name = "Settings"
Settings.Parent = Topbar
Settings.BackgroundTransparency = 1.000
Settings.Position = UDim2.new(0, 291, 0, 3)
Settings.Size = UDim2.new(0, 18, 0, 18)
Settings.ZIndex = 2
Settings.Image = "rbxassetid://3926307971"
Settings.ImageColor3 = Color3.fromRGB(207, 215, 255)
Settings.ImageRectOffset = Vector2.new(324, 124)
Settings.ImageRectSize = Vector2.new(36, 36)
Settings.MouseEnter:Connect(function()
	if not OptionsPage.Visible then
		Settings.Rotation = 45
	end
end)
Settings.MouseLeave:Connect(function()
	if not OptionsPage.Visible then
		Settings.Rotation = 0
	end
end)
OptionsPage:GetPropertyChangedSignal("Visible"):Connect(function()
	if OptionsPage.Visible then
		Settings.ImageColor3 = getgenv().caccent
		Settings:WaitForChild("Type").Value = "Accent"
		Settings.Rotation = 45
	else
		Settings.ImageColor3 = getgenv().ctext
		Settings:WaitForChild("Type").Value = "Text"
		Settings.Rotation = 0
	end
end)
Settings.MouseButton1Click:Connect(function()
	OptionsPage.Visible = not OptionsPage.Visible
end)

--Get current theme
cbackground.Value = BG.BackgroundColor3
caccent.Value = Section_2.BackgroundColor3
ctext.Value = Title.TextColor3


local pages = 0
--Create functions
--window
function createWindow(title,version)
	Title.Text = "<b>"..title.."</b><font size='12' color='rgb(125, 125, 125)'>"..version.."</font>"
end

--toggle ui
function toggleUI()
	Topbar.Visible = not Topbar.Visible
	return Topbar.Visible
end

--page
function createPage(title)
	pages = pages+1
	local newPage = Page:Clone()
	newPage.Name = pages
	newPage:WaitForChild("Section"):Destroy()
	newPage:WaitForChild("Heading").Text = "<b>"..title.."</b>"
	newPage.Parent = BG
	if pages == 1 then
		newPage.Visible = true
	else
		newPage.Visible = false
	end
	--create navbar button for page
	local btn = NavButton:Clone()
	btn.Name = pages
	btn.Text = title
	btn.Parent = Navbar
	btn.Visible = true
	if pages == 1 then
		btn:WaitForChild("Type").Value = "Accent"
		btn.BackgroundColor3 = getgenv().caccent
	else
		btn:WaitForChild("Type").Value = "Background"
		btn.BackgroundColor3 = getgenv().cbackground
	end

	--keep space at bottom
	local space = newPage:WaitForChild("Space")
	space.Parent = newPage.UIListLayout
	wait()
	space.Parent = newPage
	newPage.DescendantAdded:Connect(function(d)
		if d.Name ~= "Space" then
			local space = newPage:WaitForChild("Space")
			space.Parent = newPage:WaitForChild("UIListLayout")
			wait()
			space.Parent = newPage
		end
	end)

	return newPage
end

--section
function createSection(parent,title)
	local newSection = Section:Clone()
	newSection.Name = title
	newSection.Section.Text = title
	newSection.Parent = parent
	--clear children
	for _, v in pairs(newSection:GetChildren()) do
		if not v:IsA("UIListLayout") and not v:IsA("UICorner") and not v:IsA("StringValue") and v.Name ~= "Section" then
			v:Destroy()
		end
	end

	return newSection
end

--button
function createButton(parent,title,info,clicked)
	local newBtn = Button:Clone()
	newBtn.Name = title
	newBtn.Text = title
	newBtn.Parent = parent
	--clicked
	newBtn.MouseButton1Click:Connect(function()
		click(newBtn)
		clicked()
	end)
	--info clicked
	newBtn:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	--hover
	newBtn.MouseEnter:Connect(function()
		newBtn.BackgroundColor3 = getgenv().caccent
		newBtn.BackgroundTransparency = 0.6
	end)
	newBtn.MouseLeave:Connect(function()
		if newBtn.BackgroundTransparency > 0.59 and newBtn.BackgroundTransparency < 0.61  then
			newBtn.BackgroundColor3 = getgenv().cbackground
			newBtn.BackgroundTransparency = 0
		end
	end)

	return newBtn
end

--toggle
function createToggle(parent,title,info,clicked)
	local toggled = false
	local newToggle = Toggle:Clone()
	newToggle.Name = title
	newToggle.Text = title
	newToggle.Parent = parent
	--clicked
	newToggle.MouseButton1Click:Connect(function()
		toggled = not toggled
		if toggled then
			newToggle.ToggleStatus.BackgroundTransparency = 0
		else
			newToggle.ToggleStatus.BackgroundTransparency = 1
		end
		click(newToggle)
		clicked(toggled)
	end)
	--info clicked
	newToggle:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	--hover
	newToggle.MouseEnter:Connect(function()
		newToggle.BackgroundColor3 = getgenv().caccent
		newToggle.BackgroundTransparency = 0.6
	end)
	newToggle.MouseLeave:Connect(function()
		if newToggle.BackgroundTransparency > 0.59 and newToggle.BackgroundTransparency < 0.61  then
			newToggle.BackgroundColor3 = getgenv().cbackground
			newToggle.BackgroundTransparency = 0
		end
	end)

	return newToggle
end

--textbox
function createTextbox(parent,title,info,text)
	local newTextbox = TextBox:Clone()
	newTextbox.Name = title
	newTextbox.PlaceholderText = title
	newTextbox.Parent = parent
	--info clicked
	newTextbox:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	--text
	newTextbox.FocusLost:Connect(function(enterPressed, inputThatCausedFocusLost)
		if enterPressed then
			click(newTextbox)
			text(newTextbox.Text)
		end
	end)
	--hover
	newTextbox.MouseEnter:Connect(function()
		newTextbox.BackgroundColor3 = getgenv().caccent
		newTextbox.BackgroundTransparency = 0.6
	end)
	newTextbox.MouseLeave:Connect(function()
		if newTextbox.BackgroundTransparency > 0.59 and newTextbox.BackgroundTransparency < 0.61  then
			newTextbox.BackgroundColor3 = getgenv().cbackground
			newTextbox.BackgroundTransparency = 0
		end
	end)

	return newTextbox
end

--dropdown
function createDropdown(parent,title,info,options,chosenOption)
	local newDropdown = Dropdown:Clone()
	newDropdown.Name = title
	newDropdown:WaitForChild("Dropdown").Text = title
	--info clicked
	newDropdown.Dropdown:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	--options
	for _, v in pairs(options) do
		local option = _1:Clone()
		option.Name = "Option"
		option.Text = v
		option.Parent = newDropdown
		--option clicked
		option.MouseButton1Click:Connect(function()
			local choice = v
			click(option)
			chosenOption(v)
		end)
		--option hover
		option.MouseEnter:Connect(function()
			option.BackgroundColor3 = getgenv().caccent
			option.BackgroundTransparency = 0.6
		end)
		option.MouseLeave:Connect(function()
			if option.BackgroundTransparency > 0.59 and option.BackgroundTransparency < 0.61  then
				option.BackgroundColor3 = getgenv().cbackground
				option.BackgroundTransparency = 0
			end
		end)
	end
	newDropdown.Parent = parent
	--clicked
	newDropdown.Dropdown.MouseButton1Click:Connect(function()
		for _, v in pairs(newDropdown:GetChildren()) do
			if v.Name == "Option" then
				v.Visible = not v.Visible
			end
		end
		click(newDropdown.Dropdown)
	end)
	--hover
	newDropdown.Dropdown.MouseEnter:Connect(function()
		newDropdown.Dropdown.BackgroundColor3 = getgenv().caccent
		newDropdown.Dropdown.BackgroundTransparency = 0.6
	end)
	newDropdown.Dropdown.MouseLeave:Connect(function()
		if newDropdown.Dropdown.BackgroundTransparency > 0.59 and newDropdown.Dropdown.BackgroundTransparency < 0.61  then
			newDropdown.Dropdown.BackgroundColor3 = getgenv().cbackground
			newDropdown.Dropdown.BackgroundTransparency = 0
		end
	end)

	return newDropdown
end

--slider
function createSlider(parent,title,info,start,stop,value)
	local newSlider = Slider:Clone()
	newSlider.Name = title
	newSlider.Text = title
	newSlider.Parent = parent
	newSlider.Slide.SliderText.Text = start
	--info clicked
	newSlider:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	--slide
	local Dragging = false
	newSlider.Slide.SlideIcon.MouseButton1Down:Connect(function()
		Dragging = true
	end)
	game:GetService("UserInputService").InputChanged:Connect(function()
		if Dragging then
			local MousePos = game:GetService("UserInputService"):GetMouseLocation()+Vector2.new(0,36)
			local RelPos = MousePos-newSlider.Slide.AbsolutePosition
			local Precent = math.clamp(RelPos.X/newSlider.Slide.AbsoluteSize.X,0,1)
			newSlider.Slide.SlideIcon.Position = UDim2.new(Precent,0,0.5,0)
			local currentValue = math.floor(Precent*stop)
			currentValue = currentValue+start
			if currentValue > stop then return end
			newSlider.Slide.SliderText.Text = currentValue
			value(currentValue)
		end
	end)
	game:GetService("UserInputService").InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			Dragging = false
		end
	end)
	--hover
	newSlider.MouseEnter:Connect(function()
		newSlider.BackgroundColor3 = getgenv().caccent
		newSlider.BackgroundTransparency = 0.6
	end)
	newSlider.MouseLeave:Connect(function()
		if newSlider.BackgroundTransparency > 0.59 and newSlider.BackgroundTransparency < 0.61  then
			newSlider.BackgroundColor3 = getgenv().cbackground
			newSlider.BackgroundTransparency = 0
		end
	end)

	return newSlider
end

--keybind
function createKeybind(parent,title,info,key,pressed)
	local newKeybind = Keybind:Clone()
	newKeybind.Name = title
	newKeybind.RichText = true
	newKeybind.Text = title.." : <b><font color='rgb(255,255,255)'>"..string.sub(tostring(key),14).."</font></b>"
	newKeybind.BackgroundColor3 = getgenv().caccent
	newKeybind.BackgroundTransparency = 1
	newKeybind.Parent = parent
	local keyValue = Instance.new("StringValue")
	keyValue.Parent = newKeybind
	keyValue.Value = tostring(key)
	keyValue:GetPropertyChangedSignal("Value"):Connect(function()
		if keyValue.Value ~= "" then
			newKeybind.Text = title.." : <b><font color='rgb(255,255,255)'>"..string.sub(keyValue.Value,14).."</font></b>"
		end
	end)
	--info clicked
	newKeybind:WaitForChild("Info").MouseButton1Click:Connect(function()
		sendInfo(title,info)
	end)
	local keybindRecord = false
	--key pressed
	game:GetService("UserInputService").InputBegan:Connect(function(i,p)
		if not p then
			if keybindRecord then
				keyValue.Value = tostring(i.KeyCode)
				keybindRecord = false
			else
				if tostring(i.KeyCode) == keyValue.Value then
					click(newKeybind)
					pressed()
				end
			end
		end
	end)
	--change key
	newKeybind.MouseButton1Click:Connect(function()
		keybindRecord = true
		newKeybind.Text = title.." : <b><font color='rgb(255,255,255)'>...</font></b>"
		keyValue.Value = ""
		click(newKeybind)
	end)
	--hover
	newKeybind.MouseEnter:Connect(function()
		newKeybind.BackgroundColor3 = getgenv().caccent
		newKeybind.BackgroundTransparency = 0.6
	end)
	newKeybind.MouseLeave:Connect(function()
		if newKeybind.BackgroundTransparency > 0.59 and newKeybind.BackgroundTransparency < 0.61  then
			newKeybind.BackgroundColor3 = getgenv().caccent
			newKeybind.BackgroundTransparency = 1
		end
	end)

	return newKeybind
end
