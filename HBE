--Theme editor
--Toggle visible
for _, v in pairs(ThemeSection:GetChildren()) do
	if v:IsA("TextButton") then
		--clicked
		v.MouseButton1Click:Connect(function()
			RGB.Visible = not RGB.Visible
			CurrentlyEditing.Value = v.Name
			click(v)
		end)
		--hover
		v.MouseEnter:Connect(function()
			v.BackgroundColor3 = getgenv().caccent
			v.BackgroundTransparency = 0.6
		end)
		v.MouseLeave:Connect(function()
			if v.BackgroundTransparency > 0.59 and v.BackgroundTransparency < 0.61  then
				v.BackgroundColor3 = getgenv().cbackground
				v.BackgroundTransparency = 1
			end
		end)
	end
end
RGB.MouseLeave:Connect(function()
	RGB.Visible = false
end)

local background = Color3.fromRGB(22, 23, 36)
local accent = Color3.fromRGB(54, 78, 130)
local text = Color3.fromRGB(207, 215, 255)

local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

local selectedColor = Color3.fromHSV(1,1,1)
local colorData = {1,1,1}
local value = Value

local mouse1down = false

function changeTheme(Type,color)	
	for _, v in pairs(ScreenGui:GetDescendants()) do
		if v:FindFirstChild("Type") then
			if v.Type.Value == Type then
				if Type == "Background" then
					getgenv().cbackground = color
					if v:IsA("ImageLabel") then
						v.ImageColor3 = color
					else
						v.BackgroundColor3 = color
					end
				elseif Type == "Accent" then
					getgenv().caccent = color
					if v:IsA("UIStroke") then
						v.Color = color
					elseif v:IsA("ImageButton") then
						v.ImageColor3 = color
					else
						v.BackgroundColor3 = color
					end
				elseif Type == "Text" then
					getgenv().ctext = color
					if v:IsA("ImageButton") or v:IsA("ImageLabel") then
						v.ImageColor3 = color
					elseif v:IsA("UIStroke") then
						v.Color = color
					else
						v.TextColor3 = color
					end
				end
			end
		end
	end
	for _, v in pairs(ScreenGui:GetDescendants()) do
		if v:FindFirstChild("Type2") then
			if v.Type2.Value == Type then
				if Type == "Background" then
					getgenv().cbackground = color
					if v:IsA("ImageLabel") then
						v.ImageColor3 = color
					else
						v.BackgroundColor3 = color
					end
				elseif Type == "Accent" then
					getgenv().caccent = color
					if v:IsA("UIStroke") then
						v.Color = color
					else
						v.BackgroundColor3 = color
					end
				elseif Type == "Text" then
					getgenv().ctext = color
					if v:IsA("ImageButton") then
						v.ImageColor3 = color
					elseif v:IsA("UIStroke") then
						v.Color = color
					else
						v.TextColor3 = color
					end
				end
			end
		end
	end
end

--Preset themes
--Default
--clicked
DefaultTheme.MouseButton1Click:Connect(function()
	for _, v in pairs(Theme:GetChildren()) do
		changeTheme(v.Name,v.Value)
		if v.Name == "Background" then
			getgenv().cbackground = v.Value
			Background.Preview.BackgroundColor3 = getgenv().cbackground
		elseif v.Name == "Accent" then
			getgenv().accent = v.Value
			Accent.Preview.BackgroundColor3 = getgenv().caccent
		elseif v.Name == "Text" then
			getgenv().text = v.Value
			Text.Preview.BackgroundColor3 = getgenv().ctext
		end
	end
	click(DefaultTheme)
end)
--hover
DefaultTheme.MouseEnter:Connect(function()
	DefaultTheme.BackgroundColor3 = getgenv().caccent
	DefaultTheme.BackgroundTransparency = 0.6
end)
DefaultTheme.MouseLeave:Connect(function()
	if DefaultTheme.BackgroundTransparency > 0.59 and DefaultTheme.BackgroundTransparency < 0.61  then
		DefaultTheme.BackgroundColor3 = getgenv().cbackground
		DefaultTheme.BackgroundTransparency = 0
	end
end)


local function setColor(hue,sat,val)
	local preview = ThemeSection[CurrentlyEditing.Value].Preview

	if RGB.Visible then
		colorData = {hue or colorData[1],sat or colorData[2],val or colorData[3]}
		selectedColor = Color3.fromHSV(colorData[1],colorData[2],colorData[3])
		preview.BackgroundColor3 = selectedColor
		changeTheme(CurrentlyEditing.Value,selectedColor)
		value.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1)
	end
end

local function inBounds(frame)
	if RGB.Visible then
		local x,y = mouse.X - frame.AbsolutePosition.X,mouse.Y - frame.AbsolutePosition.Y
		local maxX,maxY = frame.AbsoluteSize.X,frame.AbsoluteSize.Y
		if x >= 0 and y >= 0 and x <= maxX and y <= maxY then
			return x/maxX,y/maxY
		end
	end
end

local function updateRGB()
	if RGB.Visible then
		if mouse1Down then
			local x,y = inBounds(RGB_2)
			if x and y then
				RGB_2:WaitForChild("Marker").Position = UDim2.new(x,0,y,0)
				setColor(1 - x,1 - y)
			end

			local x,y = inBounds(value)
			if x and y then
				value:WaitForChild("Marker").Position = UDim2.new(0.5,0,y,0)
				setColor(nil,nil,1 - y)
			end
		end
	end
end

mouse.Move:connect(updateRGB)

mouse.Button1Down:connect(function()mouse1Down = true end)
mouse.Button1Up:connect(function()mouse1Down = false end)


