getgenv().cbackground = Color3.fromRGB(22, 23, 36)
getgenv().caccent = Color3.fromRGB(54, 78, 130)
getgenv().ctext = Color3.fromRGB(207, 215, 255)

getgenv().notis = true

--Properties:

ScreenGui.Name = " "
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
ScreenGui.ResetOnSpawn = false

Circle.Name = "Circle"
Circle.Parent = Topbar
Circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Circle.BackgroundTransparency = 1.000
Circle.Visible = false
Circle.ZIndex = 11
Circle.Image = "rbxassetid://266543268"
Circle.ImageColor3 = Color3.fromRGB(0, 0, 0)
Circle.ImageTransparency = 0.500
Circle.Visible = false

--Click Effect
function click(button)
	button.BackgroundTransparency = 1
	for i = 1,10 do
		wait(0.05)
		button.BackgroundTransparency = i/10
	end
end

ScreenGui.DescendantAdded:Connect(function(d)
	wait(.5)
	if not d.Parent then return end
	if d.Name == "InfoFrame" or d.Name == "CloseInfo" or d.Name == "ConfirmClose" or d.Name == "CloseConfirmClose" then return end
	if d:FindFirstAncestor("UIStroke") then return end
	if d:IsA("TextButton") or d:IsA("TextBox") then
		if d:IsA("TextButton") then
			if d.Name == "SlideIcon" then
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Text"
				t.Parent = d
			else
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Background"
				t.Parent = d
			end
		else
			--type
			local t = Instance.new("StringValue")
			t.Name = "Type"
			t.Value = "Background"
			t.Parent = d
		end
		--type2
		local t = Instance.new("StringValue")
		t.Name = "Type2"
		t.Value = "Text"
		t.Parent = d

		if d.Name ~= "NotisBG" and d.Name ~= "Notification" then
			local uistroke = Instance.new("UIStroke")
			uistroke.Color = getgenv().caccent
			uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			uistroke.Parent = d
			--type
			local t = Instance.new("StringValue")
			t.Name = "Type"
			t.Value = "Accent"
			t.Parent = uistroke
		end
	elseif d:IsA("TextLabel") then
		--type1
		local t = Instance.new("StringValue")
		t.Name = "Type"
		t.Value = "Text"
		t.Parent = d
		if d.BackgroundTransparency ~= 1 then
			if d.Name == "Slider" then
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type2"
				t.Value = "Background"
				t.Parent = d
				--uistroke
				local uistroke = Instance.new("UIStroke")
				uistroke.Color = getgenv().caccent
				uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
				uistroke.Parent = d
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Accent"
				t.Parent = uistroke
			else
				--type2
				local t = Instance.new("StringValue")
				t.Name = "Type2"
				t.Value = "Accent"
				t.Parent = d
				--uistroke
				local uistroke = Instance.new("UIStroke")
				uistroke.Color = getgenv().caccent
				uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
				uistroke.Parent = d
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Accent"
				t.Parent = uistroke
			end
		end
	elseif d:IsA("Frame") or d:IsA("ScrollingFrame") then
		if d.Parent:IsA("TextButton") and not string.find(d.Name,"Break") then
			local uistroke = Instance.new("UIStroke")
			uistroke.Color = getgenv().ctext
			uistroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			uistroke.Parent = d
			--type
			local t = Instance.new("StringValue")
			t.Name = "Type"
			t.Value = "Text"
			t.Parent = uistroke
		end
		if d.Name == "ToggleStatus" or d.Name == "Slide" then
			--type
			local t = Instance.new("StringValue")
			t.Name = "Type"
			t.Value = "Accent"
			t.Parent = d
		end
		if d.BackgroundTransparency ~= 1 then
			if d.Name ~= "Marker" and d.Name ~= "Preview" then
				if string.find(d.Name,"Break") then
					--type
					local t = Instance.new("StringValue")
					t.Name = "Type"
					t.Value = "Accent"
					t.Parent = d
				else
					--type
					local t = Instance.new("StringValue")
					t.Name = "Type"
					t.Value = "Background"
					t.Parent = d
				end
			end
		end
	elseif d:IsA("ImageLabel") or d:IsA("ImageButton") then
		if d.Name ~= "RGB" then
			if string.find(d.Name,"Fade") then
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Background"
				t.Parent = d
			else
				--type
				local t = Instance.new("StringValue")
				t.Name = "Type"
				t.Value = "Text"
				t.Parent = d
			end
		end
	end

	--nav buttons
	if d:IsA("TextButton") and d.Parent.Name == "Navbar" then
		--clicked
		d.MouseButton1Click:Connect(function()
			d.TextColor3 = getgenv().ctext
			d.BackgroundTransparency = 0
			--set colour
			for _, v in pairs(Navbar:GetChildren()) do
				if v:IsA("TextButton") then
					v:WaitForChild("Type").Value = "Background"
					v.BackgroundColor3 = getgenv().cbackground
					if v.Name == d.Name then
						v:WaitForChild("Type").Value = "Accent"
						v.BackgroundColor3 = getgenv().caccent
					end
				end
			end
			--enable page
			for _, v in pairs(BG:GetChildren()) do
				if v:IsA("ScrollingFrame") then
					if v.Name ~= d.Name then
						v.Visible = false
					else
						v.Visible = true
					end
				end
			end
		end)
		--hover
		d.MouseEnter:Connect(function()
			if d.BackgroundColor3 ~= getgenv().caccent then
				d.BackgroundColor3 = getgenv().caccent
				d.BackgroundTransparency = 0.6
			end
		end)
		d.MouseLeave:Connect(function()
			if d.BackgroundTransparency ~= 0 then
				d.BackgroundColor3 = getgenv().cbackground
				d.BackgroundTransparency = 0
			end
		end)
	end
end)

if getgenv().key ~= k then
	Key.Name = "Key"
	Key.Parent = ScreenGui
	Key.BackgroundColor3 = Color3.fromRGB(22, 23, 36)
	Key.Position = UDim2.new(0, 528, 0, 292)
	Key.Size = UDim2.new(0, 315, 0, 54)
	--drag
	local gui = Key
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)

	UICorner.CornerRadius = UDim.new(0, 5)
	UICorner.Parent = Key

	KeyTitle.Name = "KeyTitle"
	KeyTitle.Parent = Key
	KeyTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	KeyTitle.BackgroundTransparency = 1.000
	KeyTitle.Position = UDim2.new(0, 6, 0, 0)
	KeyTitle.Size = UDim2.new(0, 145, 0, 18)
	KeyTitle.Font = Enum.Font.Nunito
	KeyTitle.Text = "<b>BottleUILib:Key</b>"
	KeyTitle.TextColor3 = Color3.fromRGB(207, 215, 255)
	KeyTitle.TextSize = 18.000
	KeyTitle.TextXAlignment = Enum.TextXAlignment.Left
	KeyTitle.TextYAlignment = Enum.TextYAlignment.Bottom
	KeyTitle.RichText = true

	Break.Name = "Break"
	Break.Parent = Key
	Break.BackgroundColor3 = Color3.fromRGB(54, 78, 130)
	Break.BorderSizePixel = 0
	Break.Position = UDim2.new(0, 0, 0, 18)
	Break.Size = UDim2.new(0, 315, 0, 1)

	Close.Name = "Close"
	Close.Parent = Key
	Close.BackgroundTransparency = 1.000
	Close.LayoutOrder = 5
	Close.Position = UDim2.new(0.935000002, 0, -0.00700000022, 0)
	Close.Size = UDim2.new(0, 18, 0, 18)
	Close.ZIndex = 2
	Close.Image = "rbxassetid://3926305904"
	Close.ImageColor3 = Color3.fromRGB(207, 215, 255)
	Close.ImageRectOffset = Vector2.new(284, 4)
	Close.ImageRectSize = Vector2.new(24, 24)
	Close.MouseButton1Click:Connect(function()
		for _, v in pairs(Key:GetChildren()) do
			if v.Name ~= "Type" and v.Name ~= "UICorner" then
				v.Visible = false
			end
		end
		game.TweenService:Create(Close, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
			ImageTransparency = 1
		}):Play()
		wait()
		game.TweenService:Create(Key, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
			Size = UDim2.new(0,0,0,0),
			Position = UDim2.new(0, Key.AbsolutePosition.X + (Key.AbsoluteSize.X / 2), 0, Key.AbsolutePosition.Y + (Key.AbsoluteSize.Y / 2))
		}):Play()
		wait(.1)
		ScreenGui:Destroy()
	end)

	KeyTextBox.Name = "KeyTextBox"
	KeyTextBox.Parent = Key
	KeyTextBox.BackgroundColor3 = Color3.fromRGB(22, 23, 36)
	KeyTextBox.BackgroundTransparency = 1.000
	KeyTextBox.Position = UDim2.new(0, 6, 0, 26)
	KeyTextBox.Size = UDim2.new(0, 302, 0, 18)
	KeyTextBox.ClearTextOnFocus = false
	KeyTextBox.Font = Enum.Font.Nunito
	KeyTextBox.PlaceholderColor3 = Color3.fromRGB(125, 125, 125)
	KeyTextBox.PlaceholderText = "Enter the key here..."
	KeyTextBox.Text = ""
	KeyTextBox.TextColor3 = Color3.fromRGB(207, 215, 255)
	KeyTextBox.TextSize = 14.000
	KeyTextBox.TextWrapped = true
	KeyTextBox.TextXAlignment = Enum.TextXAlignment.Left
	local passed = false
	KeyTextBox.FocusLost:Connect(function(enterPressed, inputThatCausedFocusLost)
		if enterPressed then
			if KeyTextBox.Text ~= "" then
				if KeyTextBox.Text == k then
					Key:Destroy()
					wait(1)
					passed = true
				else
					KeyTextBox.Text = "Invalid key"
					wait(.5)
					KeyTextBox.Text = ""
				end
			end
		end
	end)

	UICorner_2.CornerRadius = UDim.new(0, 3)
	UICorner_2.Parent = KeyTextBox

	create.Name = "create"
	create.Parent = KeyTextBox
	create.BackgroundTransparency = 1.000
	create.LayoutOrder = 5
	create.Position = UDim2.new(0.937399089, 0, -0.0171881784, 0)
	create.Size = UDim2.new(0.0595238097, 0, 1, 0)
	create.ZIndex = 2
	create.Image = "rbxassetid://3926305904"
	create.ImageColor3 = Color3.fromRGB(207, 215, 255)
	create.ImageRectOffset = Vector2.new(284, 644)
	create.ImageRectSize = Vector2.new(36, 36)

	repeat wait() until passed
end

