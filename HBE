function sendNoti(text)
	if not getgenv().notis then return end
	local noti = NotisBG:Clone()
	noti.Name = "Notification"
	noti:WaitForChild("NotisText").Text = text
	noti.Parent = NotisFrame
	noti.Visible = true
	--close
	noti.MouseButton1Click:Connect(function()
		noti:Destroy()
	end)
	for i = 1, 47 do
		wait(.125)
		if not noti:FindFirstChild("NotisBreak") then break end
		noti.NotisBreak.Size = noti.NotisBreak.Size - UDim2.new(0,3.55,0,0)
	end
	wait(.125)
	noti:Destroy()
end

NotisToggle.MouseButton1Click:Connect(function()
	getgenv().notis = not getgenv().notis
	if getgenv().notis then
		NotisToggleStatus.BackgroundTransparency = 0
		click(NotisToggle)
		sendNoti("Notifications enabled")
	else
		NotisToggleStatus.BackgroundTransparency = 1
		click(NotisToggle)
	end
end)
--hover
NotisToggle.MouseEnter:Connect(function()
	if NotisToggle.BackgroundColor3 ~= getgenv().caccent then
		NotisToggle.BackgroundColor3 = getgenv().caccent
		NotisToggle.BackgroundTransparency = 0.6
	end
end)
NotisToggle.MouseLeave:Connect(function()
	if NotisToggle.BackgroundTransparency ~= 1 then
		NotisToggle.BackgroundColor3 = getgenv().cbackground
		NotisToggle.BackgroundTransparency = 0
	end
end)

